{
  "name": "ochain",
  "version": "2.0.1",
  "release": "0",
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "bootstrap": "^4.1.2",
    "enzyme": "^2.9.1",
    "gh-pages": "^1.0.0",
    "glamor": "^2.20.30",
    "react": "^15.6.1",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-html5video": "^2.3.0",
    "react-responsive": "^1.3.4",
    "react-scrollspy": "^3.3.5",
    "react-snapshot": "^1.0.4",
    "react-sticky-header": "^0.1.9",
    "react-svg-morph": "^0.1.10",
    "react-video-cover": "^1.1.0",
    "reactstrap": "^6.3.0"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^4.0.2",
    "prettier": "^1.5.3",
    "react-scripts": "1.0.7"
  },
  "homepage": "https://ochain.com.br/",
  "lint-staged": {
    "*.js": [
      "prettier --write --single-quote true --trailing-comma all --print-width 80",
      "git add"
    ]
  },
  "private": false,
  "scripts": {
    "build": "react-scripts build",
    "deploy": "npm run build && gh-pages -d build",
    "eject": "react-scripts eject",
    "precommit": "lint-staged",
    "prettier": "./node_modules/.bin/prettier --write --single-quote true --trailing-comma all --print-width 80 --write 'src/**/*.js'",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "upg": "yarn upgrade-interactive"
  }
}
